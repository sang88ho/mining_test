tion uf_tomorrow(s_bdate) {    	var f = document.IBF;        var yyyy = s_bdate.substring(0,4);        var mm   = s_bdate.substring(5,7);        var dd   = s_bdate.substring(8);        var s_date = yyyy + mm + dd;        f.elements['조회년월일'].value = shiftDate(s_date, 0, 0, +1);        f.elements['등록회차'].value   = "00000"; //익일 거래시 회차는 "00000"으로 마지막 회차를 가져온다        f.action    = "/quics?page=C015690&cc=b028286:b028286";        f.target    = '_self';	    doAjaxCC(f);	    return false;    }            function uf_lastday(s_bdate) {    	var f = document.IBF;        var s_date = s_bdate;            f.elements['조회년월일'].value = s_date;        f.elements['등록회차'].value   = "1";        f.action    = "/quics?page=C015690/>";        f.target    = '_self';	    doAjaxCC(f);	    return false;    }             function uf_save() {        var f = document.IBF;        var pageId = "542987";        var strEftDate = "2013.12.06";        var fileTag    = "36";        var downFileName   = "환율조회(" + strEftDate + "_" + fileTag + ").xls";        var listFieldName  = "ARRAY수";        var param = "DataListFieldName=" +  listFieldName;         if (confirm("조회된 고시환율을 저장합니다.\n저장하시겠습니까?")) {            OpenStreamDownload(downFileName, pageId, param);        }    }            function uf_setFocusBtn( objBtn ){      if(objBtn != null) {        $("#strFocusBtn").val(objBtn.id);      }     }            function uf_focusIn(){    	var foucsBtnId = '';      $("#"+foucsBtnId).focus();        }    $(document).ready(function () {            	        $('#uf_save').click(function () {         	if ("" != "1") uf_save();         });                $('input[name=uf_print]').click(function () {              if ("" != "1")            	window.print();            	//printPopup("", document.IBF, "targetTable", "환율조회", 800, 600);        });                          $('input[name=dsp_mode]').toggle(        	function () {          		$('#majorDsp1').hide();        		$('#majorDsp2').hide();                $('#AllDsp1').show();                $('#AllDsp2').show();                $('input[name=dsp_mode]').val("주요통화 보기");        	},        	function () {            	$('#majorDsp1').show();            	$('#majorDsp2').show();                $('#AllDsp1').hide();	                $('#AllDsp2').hide();	                $('input[name=dsp_mode]').val("전체통화 보기");        });                 $('input[name="조회"]').click(function(){        	uf_inquery();        	return false;        });        $('input[name="저장"]').click(function(){        	uf_save();        	return false;        });        $('input[name="인쇄"]').click(function(){        	printPopup("/quics?page=C019465&cc=b028364:", document.IBF, "targetTable", "환율조회", 800, 600);        	return false;        });        $('.goLink').click(function(){        	var strNatlCd = $(this).text();        	var code = strNatlCd.substring(0,3);        	uf_goLink(code);        	return false;        });			$("#day1").change(function(){			$("#se_inqueryRound option:eq(0)").attr("selected", "selected");		});				    var foucsBtnId = '';        if(foucsBtnId != null && foucsBtnId